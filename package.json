		"concurrently": "^7.3.0",
		"nodemon": "^2.0.19"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"express-async-errors": "^3.1.1",
		"http-status-codes": "^2.2.0",
