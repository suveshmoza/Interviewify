{
	"name": "interviewify",
	"version": "1.0.0",
	"description": "interviewify keeps track of details about your job opportunities and lets you forgot those messing spreadsheets",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"server": "nodemon server --ignore client",
		"client": "npm start --prefix client",
		"start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
